{
  "name": "rescape-helpers",
  "version": "1.1.27",
  "description": "Rescape helper functions",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs  node_modules/.bin/babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es node_modules/.bin/babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development $NODE_DEBUG_OPTION rollup -c -i src/index.js -o dist/index.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production NODE_ENV=production rollup -c -i src/index.js -o dist/index.min.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:umd && yarn run build:umd:min",
    "prepare": "yarn run clean && yarn run lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g calocan/resacpe-helpers && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:calocan/rescape-helpers gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers"
  },
  "author": "Andy Likuski andy@likuski.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/rhumb-bearing": "^6.0.1",
    "@turf/rhumb-distance": "^6.0.1",
    "@turf/transform-translate": "^5.1.5",
    "apollo-client-preset": "^1.0.8",
    "d3-sankey": "../d3-sankey/",
    "d3-scale": "^2.0.0",
    "data.either": "^1.5.1",
    "graphql": "git://github.com/graphql/graphql-js.git#npm",
    "graphql-geojson": "^1.0.0",
    "graphql-tools": "^2.24.0",
    "immutable": "^4.0.0-rc.9",
    "jsdom": "^11.7.0",
    "longjohn": "^0.2.12",
    "memoize-immutable": "^3.0.0",
    "moment": "^2.22.0",
    "moment-duration-format": "^2.2.2",
    "namedtuplemap": "^1.0.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "rescape-ramda": "^1.4.23",
    "rescape-validate": "^1.2.8",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.3.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "dotenv": "5.0.1",
    "eslint": "4.19.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "5.0.0",
    "jest": "22.4.3",
    "jest-cli": "^22.4.3",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-webpack-plugin": "^0.0.2",
    "promise": "8.0.1",
    "react-dev-utils": "^5.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "whatwg-fetch": "2.0.4"
  },
  "peerDependencies": {},
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11"
            ]
          },
          "exclude": [
            "transform-async-to-generator"
          ],
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
