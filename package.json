{
  "name": "rescape-helpers",
  "version": "1.1.7",
  "description": "React NPM library starter kit based on Facebook's create react app",
  "main": "build/index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-publish-patch": "npm run build && npm run test && npm version patch && npm run publish-patch",
    "build-publish-minor": "npm run build && npm run test && npm version minor && npm run publish-minor",
    "publish-patch": "npm version patch && npm publish",
    "publsih-minor": "npm version minor && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udiliaInc/create-react-library.git"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers"
  },
  "license": "MIT",
  "dependencies": {
    "@turf/bbox": "^5.1.5",
    "@turf/bbox-polygon": "^5.1.5",
    "@turf/center": "^5.1.5",
    "@turf/rhumb-bearing": "^5.1.5",
    "@turf/rhumb-distance": "^5.1.5",
    "@turf/transform-translate": "^5.1.5",
    "apollo-client-preset": "^1.0.6",
    "apollo-link-error": "^1.0.3",
    "apollo-link-schema": "^1.0.2",
    "apollo-test-utils": "^0.3.2",
    "d3-sankey": "^0.7.1",
    "d3-scale": "^1.0.7",
    "data.either": "^1.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "enzyme-wait": "^1.0.9",
    "graphql": "^0.12.3",
    "graphql-geojson": "^1.0.0",
    "graphql-tools": "^2.18.0",
    "immutable": "^3.8.2",
    "jsdom": "^11.5.1",
    "longjohn": "^0.2.12",
    "memoize-immutable": "^3.0.0",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.0",
    "namedtuplemap": "^1.0.0",
    "numeral": "^2.0.6",
    "pretty-format": "^22.1.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react-apollo": "^2.0.4",
    "react-redux": "^5.0.6",
    "rebass": "^1.0.4",
    "redux-mock-store": "^1.4.0",
    "rescape-ramda": "^1.2.10",
    "rescape-validate": "^1.2.3",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jestsetup.js",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
