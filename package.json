{
  "name": "rescape-helpers",
  "version": "1.1.91",
  "description": "Rescape helper functions",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run clean && yarn lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g calocan/resacpe-helpers && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:calocan/rescape-helpers gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers"
  },
  "author": "Andy Likuski andy@likuski.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/meta": "^6.0.2",
    "@turf/rhumb-bearing": "^6.0.1",
    "@turf/rhumb-distance": "^6.0.1",
    "@turf/transform-translate": "^5.1.5",
    "apollo-client-preset": "^1.0.8",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "d3-scale": "^3.0.1",
    "folktale": "^2.3.0",
    "graphql": "^14.4.2",
    "graphql-geojson": "^1.0.0",
    "graphql-tools": "^4.0.5",
    "immutable": "^4.0.0-rc.9",
    "jsdom": "^15.1.1",
    "longjohn": "^0.2.12",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.3.2",
    "numeral": "^2.0.6",
    "parse-decimal-number": "^1.0.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "redux": "^4.0.4",
    "redux-debounced": "^0.5.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rescape-helpers": "^1.1.91",
    "rescape-ramda": "^1.5.69",
    "rescape-sample-data": "^1.2.58",
    "rescape-validate": "^1.2.24",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.17",
    "autoprefixer": "9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "dotenv": "8.0.0",
    "eslint": "6.1.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "4.1.0",
    "fs-extra": "8.1.0",
    "jest": "24.8.0",
    "jest-cli": "^24.8.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-webpack-plugin": "^0.1.0",
    "promise": "8.0.3",
    "react-dev-utils": "^9.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-watch": "4.3.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "whatwg-fetch": "3.0.0"
  },
  "peerDependencies": {},
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": true,
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
