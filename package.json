{
  "name": "rescape-helpers",
  "version": "1.1.71",
  "description": "Rescape helper functions",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs  node_modules/.bin/babel src --out-dir lib",
    "build:es": "BABEL_ENV=es NODE_ENV=es node $NODE_DEBUG_OPTION node_modules/.bin/rollup  -c",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development node_modules/.bin/rollup -c -out-dir dist",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production NODE_ENV=production rollup -c -out-dir dist",
    "build": "yarn run build:commonjs && yarn run build:es",
    "prepare": "yarn run clean && yarn run lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g calocan/resacpe-helpers && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:calocan/rescape-helpers gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers"
  },
  "author": "Andy Likuski andy@likuski.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/meta": "^6.0.2",
    "@turf/rhumb-bearing": "^6.0.1",
    "@turf/rhumb-distance": "^6.0.1",
    "@turf/transform-translate": "^5.1.5",
    "apollo-client-preset": "^1.0.8",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "d3-scale": "^2.1.2",
    "folktale": "^2.3.0",
    "graphql": "^14.0.2",
    "graphql-geojson": "^1.0.0",
    "graphql-tools": "^3.1.1",
    "immutable": "^4.0.0-rc.9",
    "jsdom": "^12.0.0",
    "longjohn": "^0.2.12",
    "memoize-immutable": "^3.0.0",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "namedtuplemap": "^1.0.0",
    "numeral": "^2.0.6",
    "parse-decimal-number": "^1.0.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "redux": "^4.0.0",
    "redux-debounced": "^0.5.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rescape-ramda": "^1.4.88",
    "rescape-validate": "^1.2.10",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "9.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "dotenv": "6.0.0",
    "eslint": "5.4.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "jest": "23.5.0",
    "jest-cli": "^23.5.0",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-webpack-plugin": "^0.0.2",
    "promise": "8.0.1",
    "react-dev-utils": "^5.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^5.0.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "whatwg-fetch": "2.0.4"
  },
  "peerDependencies": {},
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11"
            ]
          },
          "exclude": [
            "transform-async-to-generator"
          ],
          "modules": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ]
        ],
        "presets": []
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
